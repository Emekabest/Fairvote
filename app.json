{
  "expo": {
    "name": "Fairvote",
    "slug": "fairvote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fairvote",
    "userInterfaceStyle": "automatic",


    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.joseph.fairvote",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
      "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 150,
          "resizeMode": "cover"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "adbe43ae-da7c-4ded-b7bc-96ae24ced24a"
      }
    }
  }
}
